
public class LongestCommonPrefix {
	public static void main(String args[]) {
		String[] test = { "Hello", "Goodbye", "Etc", "Aaron", "aaron" };
		java.util.Arrays.sort(test);
		for (int i = 0; i < test.length; i++) {
			System.out.println(test[i]);
		}
	}

	public static String longestCommonPrefix(String[] strs) {
		java.util.Arrays.sort(strs);
		String longestPrefix = strs[0];
		for (int i = 1; i < strs.length; i++) {
			longestPrefix = longestCommonPrefixBetweenTwoStrings(longestPrefix, strs[i]);
		}
		return null;
	}

	public static String longestCommonPrefixBetweenTwoStrings(String a, String b) {
		StringBuffer sb = new StringBuffer();
		int shortestLength = (a.length() > b.length()) ? b.length() : a.length();
		for (int i = 0; i < shortestLength; i++) {
			if (a.charAt(i) == b.charAt(i)) {
				sb.append(a.charAt(i));
			} else if (sb.toString().isEmpty()) {
				return null;
			} else {
				return sb.toString();
			}
		}
		return null;
	}
}
